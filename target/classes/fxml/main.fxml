<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="com.agriculture.app.MainController">
    <TextField fx:id="usernameField" promptText="Username" />
    <TextField fx:id="passwordField" promptText="Password" />
    <Button fx:id="loginButton" text="Login" onAction="#handleLogin" />
    <Button fx:id="registerButton" text="Register" onAction="#handleRegister" />
</VBox>
